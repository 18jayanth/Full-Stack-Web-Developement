/* In this project we are using morgan  it will give log about the request made to the console , program returns "Hello " to client and request log details to server

import express from "express";
import morgan from "morgan"

const app = express();
const port = 3000;
app.use(morgan("combined"));
app.get("/", (req, res) => {
  res.send("Hello");
});

app.listen(port, () => {
  console.log(`Listening on port ${port}`);
});
